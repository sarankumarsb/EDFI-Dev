@model IGranularMetric
<tr id="vMetric@(Model.MetricVariantId)"  data-metric-id="@(Model.MetricId)" data-metric-variant-id="@(Model.MetricVariantId)">
    <td headers="MetricName@(this.GetMetricRenderingContextValue("ParentContainerMetricId"))" data-item-type="DisplayName" title="@Model.ToolTip">
        <p class="arrow">
            <i class="icon-blank"></i>
            <a name="m@(Model.MetricVariantId)"></a>
            <span class="metricId">@(Model.MetricId)-@(Model.MetricVariantType)</span>
            @(Model.DisplayName)
            @if (!String.IsNullOrWhiteSpace(Model.Context))
            {
                <span>&nbsp;(@Model.Context)</span>
            }
            @Html.Partial("MetricFootnoteLabelSuperText", Model.Footnotes)
                
            @Html.Partial("UnderConstruction", (MetricBase)Model)
        </p>
    </td>
    <td headers="MetricEnrollment@(this.GetMetricRenderingContextValue("ParentContainerMetricId"))" data-item-type="Enrollment">
        @if (Model.Values["Taken"]!=null)
        {
            @(Model.Values["Taken"].ToString())
        }
    </td>
    <td headers="MetricCompletion@(this.GetMetricRenderingContextValue("ParentContainerMetricId"))" data-item-type="Completion">
        @if (Model.Values["Completion"] != null)
        {
            @(Model.Values["Completion"].ToString())
        }
    </td>
    <td headers="MetricMastery@(this.GetMetricRenderingContextValue("ParentContainerMetricId"))" data-item-type="Mastery">
        @Html.Partial("StatusLabel", Model.State)
    </td>
    <td headers="MetricDetails@(this.GetMetricRenderingContextValue("ParentContainerMetricId"))" data-item-type="ContextMenu">
        @Html.Partial("ContextMenu", (MetricBase)Model)
    </td>
</tr>