@model AggregateMetric
<tr id="vMetric@(Model.MetricVariantId)" style="height: 35px;">
    <td title="@Model.ToolTip" class="bulleted-metric bulleted-metric-indented">
        <i class="icon-right-circled"></i>
        <p class="title">
            <a href="@(Url.Content(Model.Url))" id="vName@(Model.MetricVariantId)">
                @(Model.DisplayName)
            </a>
            @if (!string.IsNullOrEmpty(Model.Context))
            {
                <span id="vContext@(Model.MetricVariantId)">&nbsp;(@(Model.Context))</span>
            }
            <span id="vDescription@(Model.MetricVariantId)" class="description">
                @Model.Description
            </span>
        </p>
    </td>
    <td>
        @Html.Partial("AggregateSummary", Model)
    </td>
    <td>
        @Html.Partial("AggregateStatus", Model)
    </td>
</tr>
