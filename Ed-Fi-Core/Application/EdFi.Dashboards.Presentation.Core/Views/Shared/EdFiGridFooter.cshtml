@model EdFi.Dashboards.Presentation.Core.Models.Shared.EdFiGridModel
 @if (Model.LegendViewNames != null && Model.LegendViewNames.Count > 0)
 {
            <div id="@(Model.GridName)-EdFiGrid-legend" class="EdFiGrid-legend">
                <table id="@(Model.GridName)-EdFiGrid-legend-table" class="EdFiGrid-legend-table" cellpadding="0" cellspacing="0" border="0">
                    <tbody>
                        <tr>
                            @foreach (var legendView in Model.LegendViewNames)
                            {
                                @Html.Partial(string.Format("~/Views/Shared/EdFiGridLegend{0}.cshtml", legendView))
                            }
                        </tr>
                    </tbody>
                </table>
            </div>
 }
@if (Model.MetricFootnotes != null)
{
    <div id="@(Model.GridName)-EdFiGrid-footnotes" class="EdFiGrid-footnotes">
        @Html.Partial("~/Views/MetricTemplates/Shared/MetricFootnoteLabelSuperText.cshtml", Model.MetricFootnotes, new ViewDataDictionary { { "FootnoteVisible", true } })
    </div>
}
