@using EdFi.Dashboards.Presentation.Core.Models.Shared
@{
    var legendItems = new List<EdFiGridLegendModel>
        {
            new EdFiGridLegendModel
                {
                    CssClass = EdFiWebFormsDashboards.Site.Common.ThemeImage("StatusLabel/RoundVeryGood.png").Resolve(),
                    Text = "100% Correct",
                    Alt = "Star"
                },
            new EdFiGridLegendModel
                {
                    CssClass = EdFiWebFormsDashboards.Site.Common.ThemeImage("StatusLabel/RoundGood.png").Resolve(),
                    Text = "70% to 99% Correct",
                    Alt = "Green"
                },
            new EdFiGridLegendModel
                {
                    CssClass = EdFiWebFormsDashboards.Site.Common.ThemeImage("StatusLabel/RoundLow.png").Resolve(),
                    Text = "< 70% Correct",
                    Alt = "Red"
                }
        };
}

@foreach (var entry in @legendItems)
{
    <td class="EdFiGrid-legend-table-image">
        <img src="@entry.CssClass" alt="@entry.Alt" />
    </td>
    <td class="EdFiGrid-legend-table-text">
        @entry.Text
    </td>
}