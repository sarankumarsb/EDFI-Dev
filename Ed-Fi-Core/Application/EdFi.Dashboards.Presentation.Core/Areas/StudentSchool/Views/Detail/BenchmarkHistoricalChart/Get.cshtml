@using EdFi.Dashboards.Core.Providers.Context
@using EdFi.Dashboards.Presentation.Architecture.Mvc.Extensions
@using EdFi.Dashboards.Presentation.Core.Models.Shared
@using EdFi.Dashboards.Presentation.Core.Models.Shared.Detail
@using EdFi.Dashboards.Resources.Navigation.Mvc

@model HistoricalChartModel

@{
    var studentUSI = EdFiDashboardContext.Current.StudentUSI.GetValueOrDefault();
}

<script>
    analyticsManager.trackDrilldown('StudentBenchmarkHistoricalChart', '@(Model.MetricVariantId)', '@(studentUSI)');
</script>

@{Html.RenderPartial("HistoricalChart", model: Model);}