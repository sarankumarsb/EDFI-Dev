@using EdFi.Dashboards.Core.Providers.Context
@using EdFi.Dashboards.Presentation.Web.Architecture.HtmlHelperExtensions
@using System.Web.UI.DataVisualization.Charting
@model EdFi.Dashboards.Resources.Models.Charting.ChartData

@{
    var schoolId = EdFiDashboardContext.Current.SchoolId.GetValueOrDefault();
    var metricVariantId = EdFiDashboardContext.Current.MetricVariantId.GetValueOrDefault();
}

<script type="text/javascript" language="javascript">
    analyticsManager.trackDrilldown('SchoolHistoricalLearningObjectivesChart', '@(metricVariantId)', '@(schoolId)');
</script>
<p>@Model.Goal</p>
@Html.BarChart(Model, 740, 250, 0, 0, null, null, false, SeriesChartType.Column, false)
